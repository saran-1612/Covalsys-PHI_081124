<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/transparent"
            android:orientation="vertical">

            <androidx.core.widget.NestedScrollView
                android:id="@+id/bottomSheet"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fillViewport="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="@dimen/dimen5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="0dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="7">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/scanLayout"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etScan"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:hint="@string/b_scan"
                                    android:imeOptions="actionUnspecified"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:padding="@dimen/dimen2dp"
                            android:layout_weight="3">

                            <Button
                                android:id="@+id/doc_scan"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dimen50dp"
                                android:background="?attr/btnBackground"
                                android:text="@string/b_scan"
                                android:textColor="@color/white" />

                        </LinearLayout>
                    </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="0dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="@dimen/dimen0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/layDocDate"
                            style="@style/TextInputLayoutStyle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen5dp"
                            android:layout_marginBottom="-5dp"
                            android:paddingStart="@dimen/dimen5dp"
                            android:paddingEnd="@dimen/dimen5dp"
                            app:boxStrokeColor="?attr/colorPrimary"
                            app:boxStrokeWidth="1dp">

                            <EditText
                                android:id="@+id/etDocDate"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dimen50dp"
                                android:hint="@string/b_doc_date"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:imeOptions="actionUnspecified"
                                android:focusable="false"
                                android:inputType="date"
                                android:maxLines="1"
                                android:padding="@dimen/dimen5dp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="@dimen/dimen0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/etDealerNameLayout"
                            style="@style/TextInputLayoutStyle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen5dp"
                            android:layout_marginBottom="-5dp"
                            android:paddingStart="@dimen/dimen5dp"
                            android:paddingEnd="@dimen/dimen5dp"
                            app:boxStrokeColor="?attr/colorPrimary"
                            app:boxStrokeWidth="1dp">

                            <EditText
                                android:id="@+id/etOutPassNo"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dimen50dp"
                                android:hint="@string/b_out_pass_no"
                                android:imeOptions="actionUnspecified"
                                android:inputType="text|numberDecimal"
                                android:maxLines="1"
                                android:padding="@dimen/dimen5dp"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="0dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="@dimen/dimen0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputLayout
                            style="@style/SpinnerInputLayoutStyle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen5dp"
                            android:layout_marginBottom="-5dp"
                            android:paddingStart="@dimen/dimen5dp"
                            android:paddingEnd="@dimen/dimen5dp"
                            app:boxStrokeColor="?attr/colorPrimary"
                            app:boxStrokeWidth="1dp"
                            android:hint="@string/b_type">

                            <AutoCompleteTextView
                                android:id="@+id/filledType"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dimen50dp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                                android:inputType="text"/>

                        </com.google.android.material.textfield.TextInputLayout>

                    </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layOutPassDate"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etOutPassDate"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:hint="@string/b_out_pass_date"
                                    android:imeOptions="actionUnspecified"
                                    android:focusable="false"
                                    android:inputType="date"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="0dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="@dimen/dimen0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/layCusType"
                            style="@style/SpinnerInputLayoutStyle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen5dp"
                            android:layout_marginBottom="-5dp"
                            android:paddingStart="@dimen/dimen5dp"
                            android:paddingEnd="@dimen/dimen5dp"
                            app:boxStrokeColor="?attr/colorPrimary"
                            app:boxStrokeWidth="1dp"
                            android:hint="@string/b_cus_type">

                            <AutoCompleteTextView
                                android:id="@+id/filledCusType"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dimen50dp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                                android:inputType="text"/>

                        </com.google.android.material.textfield.TextInputLayout>

                    </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/laySecurityCode"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etSecurityCode"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:hint="@string/b_sec_code"
                                    android:imeOptions="actionUnspecified"
                                    android:inputType="text|numberDecimal"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="0dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="@dimen/dimen0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/layPriority"
                            style="@style/SpinnerInputLayoutStyle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen5dp"
                            android:layout_marginBottom="-5dp"
                            android:paddingStart="@dimen/dimen5dp"
                            android:paddingEnd="@dimen/dimen5dp"
                            app:boxStrokeColor="?attr/colorPrimary"
                            app:boxStrokeWidth="1dp"
                            android:hint="@string/b_priority">

                            <AutoCompleteTextView
                                android:id="@+id/filledPriority"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dimen50dp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                                android:inputType="text"/>

                        </com.google.android.material.textfield.TextInputLayout>

                    </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/laySecName"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etSecName"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:hint="@string/b_sec_name"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>
                </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="0dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layEwayBillNo"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etEwayBillNo"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:hint="@string/b_eway_bill_no"
                                    android:inputType="text|number"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layAreaOfWork"
                                style="@style/SpinnerInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp"
                                android:hint="@string/b_area_work">

                                <AutoCompleteTextView
                                    android:id="@+id/filledWork"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                                    android:inputType="text"/>

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>
                </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="0dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layVehicleNo"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etVehicleNo"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:hint="@string/b_vehicle_no"
                                    android:inputType="text|number"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layStatus"
                                style="@style/SpinnerInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp"
                                android:hint="@string/b_status">

                                <AutoCompleteTextView
                                    android:id="@+id/filledStatus"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                                    android:inputType="text"/>

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>
                </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="0dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layRemark"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etRemark"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:hint="@string/b_remark"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:imeOptions="actionUnspecified"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layDocTotal"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etDocTotal"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:hint="@string/b_total_doc"
                                    android:imeOptions="actionUnspecified"
                                    android:inputType="numberDecimal"
                                    android:focusable="false"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="0dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layNoOfDoc"
                                style="@style/TextInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp">

                                <EditText
                                    android:id="@+id/etNoOfDoc"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:hint="@string/b_no_of_doc"
                                    android:inputType="number"
                                    android:focusable="false"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimen5dp"
                                    android:layout_marginBottom="@dimen/dimen5dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/layValidation"
                                style="@style/SpinnerInputLayoutStyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_marginBottom="-5dp"
                                android:paddingStart="@dimen/dimen5dp"
                                android:paddingEnd="@dimen/dimen5dp"
                                app:boxStrokeColor="?attr/colorPrimary"
                                app:boxStrokeWidth="1dp"
                                android:hint="@string/b_dc_validation">

                                <AutoCompleteTextView
                                    android:id="@+id/filledValidation"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/dimen50dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                                    android:inputType="text"/>

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimen20dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="@dimen/dimen0dp"
                            android:layout_weight="1"
                            android:layout_margin="@dimen/dimen2dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:orientation="vertical">

                            <Button
                                android:id="@+id/btnCancel"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/btnBackground"
                                android:text="@string/cancel"
                                android:textColor="@color/white" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_margin="@dimen/dimen2dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:orientation="vertical">

                            <Button
                                android:id="@+id/btnLoad"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/btnBackground"
                                android:text="@string/b_load"
                                android:textColor="@color/white" />

                        </LinearLayout>
                    </LinearLayout>

                    <View
                        android:id="@+id/extraSpace"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                </LinearLayout>
            </androidx.core.widget.NestedScrollView>
        </LinearLayout>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</layout>